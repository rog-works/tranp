{%- set iterates = namespace(at='') -%}
{%- for statement in statements[0].split('\n') -%}
{%- if statement.startswith('return ') -%}
{%- set iterates = statement[('return ' | length):-1] -%}
{%- set _return_type = i18n('classes', 'dict') + '<' + return_type[('ItemsView<' | length):-1] + '>::' + i18n('cpp', 'map.iterator') -%}
{{ _return_type }} {{ i18n('cpp', 'map.begin') }}() { return ({{ iterates }}).{{ i18n('cpp', 'map.begin') }}(); }
{{ _return_type }} {{ i18n('cpp', 'map.end') }}() { return ({{ iterates }}).{{ i18n('cpp', 'map.end') }}(); }
{%- endif -%}
{%- endfor -%}