{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: unittest discover",
			"type": "debugpy",
			"request": "launch",
			"module":"unittest",
			"args": ["discover", "tests/"],
			"cwd": "${workspaceFolder}",
			"windows": {
				"env": {
					"PYTHONPATH": "${workspaceFolder};${workspaceFolder}/vendor",
					"PYTHONDONTWRITEBYTECODE": "1",
				},
			},
			"linux": {
				"env": {
					"PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/vendor",
					"PYTHONDONTWRITEBYTECODE": "1",
				},
			},
			"console": "integratedTerminal",
			"justMyCode": true,
		},
		{
			"name": "Python: unittest file",
